<h1>Listing key_terms</h1>

<table>
  <tr>
    <th>Term</th>
    <th>Meaning</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @key_terms.each do |key_term| %>
  <tr>
    <td><%= key_term.term %></td>
    <td><%= key_term.meaning %></td>
    <td><%= link_to 'Show', key_term %></td>
    <td><%= link_to 'Edit', edit_key_term_path(key_term) %></td>
    <td><%= link_to 'Destroy', key_term, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Key term', new_key_term_path %>
