<!DOCTYPE html>
<html>
<head>
  <title>CMS</title>
  <%= stylesheet_link_tag :all, 'jquery-ui-1.8.10.custom' %>
  <%= javascript_include_tag :defaults, "jquery.min", "jquery-ui.min", 'jquery-ui-1.8.10.custom.min' %>
  <%= csrf_meta_tag %>




</head>
<body>
    <%= render :partial => '/navigation_header' %>

    <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>


<%= yield %>

</body>
</html>
